<%= form_with url: search_path, local: true, method: :get do |form| %>
  <%= form.hidden_field :form_assembly_url, value: ENV['FORM_ASSEMBLY_URL'] %>
  <%= form.text_field :name, placeholder: 'Nome' %>
  <%= form.text_field :email, placeholder: 'Email' %>
  <br/>
  Estado
  <%= form.collection_select :state_id, @states, :id, :name, prompt: 'Estado' %>
  <br/>
  Cidade
  <%= form.collection_select :city_name, @cities, :name, :name, prompt: 'Cidade' %>
  <br/>
  Dependencia Administrativa
  <%= form.select :administration, options_for_select(MecSchool::ADMINISTRATIONS), prompt: 'Dependencia Administrativa' %>
  <br/>
  <%= form.text_field :school, placeholder: 'Escola', data: { behavior: 'search' } %>
  <%= form.submit 'Ir para o questionÃ¡rio' %>
<% end %>